{
  "name": "effectra/laravel-model-operations",
  "description": "A Laravel package for model operations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    { "name": "Effectra", "email": "info@effectra.com" },
    { "name": "Mohammed Taha [BMT]", "email": "info@mohammedtaha.net" }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.49",
    "illuminate/database": "^10.49",
    "spatie/laravel-package-tools": "^1.92"
  },
  "autoload": {
    "psr-4": {
      "Effectra\\LaravelModelOperations\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Effectra\\LaravelModelOperations\\Providers\\LaravelModelOperationsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "pestphp/pest": "^3.8",
    "phpstan/phpstan": "^1.8"
  }
}
